{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "yarn build",
    "beforeDevCommand": "yarn start",
    "devPath": "http://localhost:3000",
    "distDir": "../src"
  },
  "package": {
    "productName": "sm-init-app-v2",
    "version": "1.2.5"
  },
  "tauri": {
    "allowlist": {
      "shell": {
        "all": true,
        "execute": true,
        "open": true,
        "scope": [],
        "sidecar": false
      },
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "identifier": "com.tauri.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": "default-src 'self' blob: data: filesystem: ws: http: https: 'unsafe-eval' 'unsafe-inline'; script-src 'self'; style-src 'self'"
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "label": "main",
        "width": 1200,
        "height": 760,
        "resizable": false,
        "fullscreen": false,
        "decorations": true
      }
    ]
  }
}
